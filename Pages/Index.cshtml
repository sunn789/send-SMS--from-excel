@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}


<h2>Upload Excel</h2>

<form method="post" enctype="multipart/form-data">
    <div>
        <label for="file">Select Excel File:</label>
        <input type="file" id="file" name="file" />
    </div>
    <button type="submit">Upload</button>
</form>

@if (Model.Persons != null && Model.Persons.Any())
{
    <h3>اطلاعات بازگذاری شده</h3>
    <table>
        <thead>
            <tr>
                <th>title</th>
                <th>full name</th>
                <th>Position</th>
                <th>Position</th>
                <th>Position</th>
                <th>Position</th>
                <th>Position</th>
                <th>Position</th>
                <th>Position</th>
                <th>Position</th>
                <th>Position</th>
                <th>Position</th>
                <th>Position</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var employee in Model.Persons)
            {
                <tr>
                    <td>@employee.Title</td>
                    <td>@employee.FullName</td>
                    <td>@employee.PhoneNumber</td>
                    <td>@employee.TehToKerDateTime</td>
                    <td>@employee.TehToKerDateTime</td>
                    <td>@employee.TerminalNumber</td>
                    <td>@employee.Airline</td>
                    <td>@employee.FlightNumber</td>
                    <td>@employee.FightTime</td>
                    <td>@employee.ExiteDate</td>
                    <td>@employee.ExitAirline</td>
                    <td>@employee.ExitFilghtNumber</td>
                    <td>@employee.ExiteTime</td>
              
                </tr>
            }
        </tbody>
    </table>
}